    "mongodb": "^6.7.0",
    "mongoose": "^8.4.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "ndarray": "^1.0.19",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.4",
    "nodemon": "^3.0.1",
    "openmeteo": "^1.1.4",
    "path": "^0.12.7",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.5",
    "slugify": "^1.6.6",
    "uniqid": "^5.4.0"
  },
